 ([yshift=25pt,xshift=-20pt]current page.north east)-- 
 ([yshift=70pt,xshift=-20pt]current page.south east)-- 
 ([yshift=70pt,xshift=20pt]current page.south west)--cycle; 
 \end{tikzpicture}
 }



